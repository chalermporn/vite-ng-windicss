{
  "name": "vite-ng-windicss",
  "version": "0.0.0",
  "scripts": {
    "ngcnc": "ngcc --tsconfig ./tsconfig.app.json && ngc --project tsconfig.app.json",
    "watch-ngc": "ngc --project tsconfig.app.json --watch",
    "watch-vite": "vite serve",
    "start": "pnpm ngcnc && concurrently 'pnpm:watch-ngc' 'pnpm:watch-vite'",
    "build": "pnpm ngcnc && vite build",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.13",
    "@angular/common": "~12.2.13",
    "@angular/compiler": "~12.2.13",
    "@angular/core": "~12.2.13",
    "@angular/forms": "~12.2.13",
    "@angular/platform-browser": "~12.2.13",
    "@angular/platform-browser-dynamic": "~12.2.13",
    "@angular/router": "~12.2.13",
    "rxjs": "~6.6.7",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.13",
    "@angular/cli": "~12.2.13",
    "@angular/compiler-cli": "~12.2.13",
    "@types/jasmine": "~3.8.2",
    "@types/node": "^12.20.37",
    "concurrently": "^6.4.0",
    "jasmine-core": "~3.8.0",
    "karma": "~6.3.9",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "~1.7.0",
    "typescript": "~4.3.5",
    "vite": "^2.6.14"
  }
}
